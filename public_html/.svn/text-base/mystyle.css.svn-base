h1.header {font-family:Georgia;color:#8ce1f5;font-size:40px;margin-bottom:0.2em;margin-top:0.2em;text-shadow: 1px 1px 2px black, 0 0 1em black, 0 0 0.2em black;}
h2.header{font-family:Georgia;color:#ffffff;margin-top:0px;margin-bottom:1em;font-size:18px;}

h1.entry{font-family:Georgia;color:#29aae1;font-size:24px;margin-top:0px;margin-bottom:0.5em;}
h2.entry{font-family:Georgia;color:#29aae1;font-size:20px;margin-top:0px;margin-bottom:0px}

h1.forum{font-family:Georgia;color:#29aae1;;font-size:24px;margin-bottom:0.2em;margin-top:0.2em;}

p.entry {font-family:Georgia;margin-top:0em;margin-bottom:0em;}
p.footer {font-family:Georgia;margin-top:0em;margin-bottom:0em;color:#29aae1;}
p+p.entry {margin-top: 1em;margin-bottom: 0em;}

body {background-color:#2d3037;}

table.header{width:80%;border:0px;padding:0px}
table.menu{width:80%;border:0px;background-color:#ffffff;}
table.entry{width:80%;border:0px;background-color:#ffffff;padding:5px}
table.forum{width:80%;border:0px;background-color:#ffffff;border-collapse:collapse}
table.forum td{border-bottom:5px solid #2d3037;padding:5px;}

a.etitle:link {text-decoration:none;color:#29aae1;font-family:Georgia;font-size:20px;margin-top:0px;margin-bottom:0.5em;font-weight:bold;}
a.etitle:visited {text-decoration:none;color:#29aae1;font-family:Georgia;font-size:20px;margin-top:0px;margin-bottom:0.5em;font-weight:bold;}
a.etitle:hover {text-decoration:underline;color:#8ce1f5;font-family:Georgia;font-size:20px;margin-top:0px;margin-bottom:0.5em;font-weight:bold;}
a.etitle:active {text-decoration:underline;color:#29aae1;font-family:Georgia;font-size:20px;margin-top:0px;margin-bottom:0.5em;font-weight:bold;}

a.htitle:link {text-decoration:none;}
a.htitle:visited {text-decoration:none;}
a.htitle:hover{text-decoration:none;}
a.htitle:active{text-decoration:none;}

a.header:link {text-decoration:none;color:#8ce1f5;font-family:Georgia;}
a.header:visited {text-decoration:none;color:#29aae1;font-family:Georgia;}
a.header:hover {text-decoration:underline;color:#ffffff;font-family:Georgia;}
a.header:active {text-decoration:underline;color:#ffffff;font-family:Georgia;}

a.entry:link {text-decoration:none;color:#000000;font-family:Georgia;}
a.entry:visited {text-decoration:none;color:#2d3037;font-family:Georgia;}
a.entry:hover {text-decoration:underline;color:#999999;font-family:Georgia;}
a.entry:active {text-decoration:underline;color:#999999;font-family:Georgia;}

a.menu:link {text-decoration:none;font-weight:bold;color:#000000;font-family:Georgia;}
a.menu:visited {text-decoration:none;font-weight:bold;color:#2d3037;font-family:Georgia;}
a.menu:hover {text-decoration:underline;font-weight:bold;color:#999999;font-family:Georgia;}
a.menu:active {text-decoration:underline;font-weight:bold;color:#999999;font-family:Georgia;}

input {background-color:white;border: 1px solid;color:black;
}

input.search {background-color:white;border: 0px solid;height:20px;width:167%;color:black;
}
